{
  "swagger": "2.0",
  "info": {
    "title": "More API",
    "description": "More API with express",
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/register": {
      "post": {
        "tags": [
          "admins"
        ],
        "description": "관리자 등록",
        "parameters": [
          {
            "name": "",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "관리자"
                },
                "email": {
                  "type": "string",
                  "example": "admin"
                },
                "password": {
                  "type": "string",
                  "example": "1234"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "관리자 등록 성공",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "관리자를 생성하였습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "body 또는 params를 입력받지 못한 경우",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "데이터 형식이 올바르지 않습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": [
          "admins"
        ],
        "description": "관리자 로그인",
        "parameters": [
          {
            "name": "",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "example": "admin"
                },
                "password": {
                  "type": "string",
                  "example": "1234"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "관리자 로그인 성공",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "관리자 아이디로 로그인했습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "body 또는 params를 입력받지 못한 경우",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "email 또는 password 입력값이 올바르지 않습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/info": {
      "get": {
        "tags": [
          "admins"
        ],
        "description": "관리자 정보 조회",
        "parameters": [],
        "responses": {
          "201": {
            "description": "관리자 정보 조회 성공",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "example": "uuid"
                      },
                      "name": {
                        "type": "string",
                        "example": "관리자"
                      },
                      "email": {
                        "type": "string",
                        "example": "admin"
                      },
                      "password": {
                        "type": "string",
                        "example": "1234"
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    }
  }
}